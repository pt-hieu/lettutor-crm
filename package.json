{
  "name": "lettutor-crm",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "postgres": "docker run -e POSTGRES_PASSWORD=postgres -d -p 5432:5432 --name postgres postgres:alpine",
    "db": "docker exec -it postgres psql -U postgres",
    "athena": "cd athena && terraform",
    "tool": "cd athena && terraformer",
    "build:script": "tsc ./script.ts",
    "up": "node ./script.js -b",
    "format": "prettier --write {artemis,apollo,zeus,ares}/**/*.{ts,tsx}"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^17.0.13",
    "@types/yargs": "^17.0.8",
    "concurrently": "^7.0.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "yargs": "^17.3.1"
  }
}
